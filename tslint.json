{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier",
    "tslint-no-commented-code-rule"
  ],
  "jsRules": {
    "object-literal-sort-keys": false
  },
  "rules": {
    "array-bracket-spacing": [
      false
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "arrow-return-shorthand": [
      true
    ],
    "await-promise": true,
    "ban-types": [
      true,
      [
        "Object",
        "Use {} instead."
      ]
    ],
    "binary-expression-operand-order": true,
    "brace-style": [
      true,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": [
      true,
      "ignore-same-line"
    ],
    "cyclomatic-complexity": [
      true,
      40
    ],
    "eofline": true,
    "forin": true,
    "import-spacing": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "jsx-alignment": false,
    "jsx-boolean-value": [
      true,
      "never"
    ],
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-space-before-trailing-slash": true,
    "label-position": true,
    "lines-between-class-members": true,
    "max-line-length": [
      true,
      180
    ],
    "member-access": false,
    "member-ordering": [
      true,
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-arg": true,
    "no-async-without-await": true,
    "no-bitwise": false,
    "no-commented-code": [
      true,
      {
        "minLineCount": 2,
        "ignoredCommentRegex": "^(\\w+$|TODO|FIXME)"
      }
    ],
    "no-consecutive-blank-lines": [
      true
    ],
    "no-console": [
      true,
      "log",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-for-in-array": true,
    "no-implicit-dependencies": [
      false,
      "dev"
    ],
    "no-inferrable-types": [
      true
    ],
    "no-invalid-this": [
      true
    ],
    "no-magic-numbers": false,
    "no-multi-spaces": [
      true
    ],
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-submodule-imports": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": [
      true,
      "allow-fast-null-checks"
    ],
    "no-var-keyword": true,
    "object-curly-spacing": [
      true,
      "always"
    ],
    "object-literal-shorthand": true,
    "object-literal-sort-keys": false,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "ordered-imports": [
      true
    ],
    "prefer-const": true,
    "prefer-function-over-method": true,
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ],
    "radix": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "space-before-function-paren": [
      true,
      {
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "space-in-parens": [
      true,
      "never"
    ],
    "switch-default": true,
    "ter-arrow-body-style": [
      true
    ],
    "ter-func-call-spacing": [
      true,
      "never"
    ],
    "ter-no-irregular-whitespace": [
      true
    ],
    "ter-no-sparse-arrays": [
      true
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast",
      "check-preblock"
    ]
  },
  "rulesDirectory": []
}
